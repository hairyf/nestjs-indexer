{
  "name": "nestjs-indexer",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@10.25.0",
  "description": "A distributed cursor indexing and scheduling framework. Supports atomic stepping, concurrency control, and automatic failure retries.",
  "author": "Hairyf <wwu71063@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/hairyf/nestjs-indexer#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hairyf/nestjs-indexer.git"
  },
  "bugs": "https://github.com/hairyf/nestjs-indexer/issues",
  "keywords": [],
  "sideEffects": false,
  "publishConfig": {
    "exports": {
      ".": "./dist/index.mjs",
      "./package.json": "./package.json"
    },
    "main": "./dist/index.mjs",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.mts"
  },
  "exports": {
    ".": "./dist/index.mjs",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "lint": "eslint",
    "prepublishOnly": "nr build",
    "release": "bumpp",
    "start": "tsx src/index.ts",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "typecheck": "tsc",
    "prepare": "simple-git-hooks"
  },
  "peerDependencies": {
    "@nestjs/common": "^8.0.0 || ^9.0.0 || ^10.0.0 || ^11.0.0",
    "@nestjs/core": "^8.0.0 || ^9.0.0 || ^10.0.0 || ^11.0.0",
    "ioredis": "^5.0.0",
    "redlock-universal": "^0.8.0",
    "reflect-metadata": "^0.1.13 || ^0.2.0"
  },
  "dependencies": {
    "@hairy/utils": "catalog:runtime",
    "redlock-universal": "catalog:runtime",
    "unstorage": "catalog:inlined"
  },
  "devDependencies": {
    "@antfu/eslint-config": "catalog:cli",
    "@antfu/ni": "catalog:cli",
    "@antfu/utils": "catalog:inlined",
    "@nestjs/common": "catalog:runtime",
    "@nestjs/core": "catalog:runtime",
    "@nestjs/testing": "catalog:runtime",
    "@oxc-node/core": "catalog:runtime",
    "@types/ioredis-mock": "catalog:testing",
    "@types/node": "catalog:types",
    "@vitest/coverage-v8": "catalog:testing",
    "bumpp": "catalog:cli",
    "eslint": "catalog:cli",
    "ioredis": "catalog:runtime",
    "ioredis-mock": "catalog:testing",
    "lint-staged": "catalog:cli",
    "publint": "catalog:cli",
    "reflect-metadata": "catalog:inlined",
    "simple-git-hooks": "catalog:cli",
    "tinyexec": "catalog:testing",
    "tsdown": "catalog:cli",
    "tsx": "catalog:cli",
    "typescript": "catalog:cli",
    "vite": "catalog:cli",
    "vitest": "catalog:testing",
    "vitest-package-exports": "catalog:testing",
    "yaml": "catalog:testing"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm i --frozen-lockfile --ignore-scripts --offline && npx lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
